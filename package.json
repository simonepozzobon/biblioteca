{
    "name": "biblioteca",
    "description": "Client di consultazione per la Biblioteca di Morando di Fondazione Cineteca Italiana di Milano",
    "author": "Simone Pozzobon",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "electron:build": "vue-cli-service electron:build",
        "electron:serve": "vue-cli-service electron:serve",
        "postinstall": "electron-builder install-app-deps",
        "postuninstall": "electron-builder install-app-deps",
        "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=dist --flatten"
    },
    "main": "background.js",
    "dependencies": {
        "core-js": "^2.6.5",
        "element-ui": "^2.9.1",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.8.0",
        "@vue/cli-plugin-eslint": "^3.8.0",
        "@vue/cli-service": "^3.8.0",
        "axios": "^0.19.0",
        "babel-eslint": "^10.0.1",
        "dotenv": "^8.0.0",
        "electron": "^5.0.4",
        "electron-builder": "^20.43.0",
        "electron-icon-builder": "^1.0.0",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "node-sass": "^4.12.0",
        "sass-loader": "^7.1.0",
        "vue-cli-plugin-electron-builder": "^1.3.4",
        "vue-router": "^3.0.6",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}